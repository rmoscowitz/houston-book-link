  "private": true,
  "main": "server/index.js",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bookshelf": "^0.14.2",
    "bootstrap": "^4.3.1",
    "knex": "^0.16.3",
    "pg": "^7.9.0",
    "react": "^16.8.6",
    "react-debounce-input": "^3.2.0",
    "react-dom": "^16.8.6",
    "react-foundation": "^0.9.6",
    "react-ga": "^2.5.7",
    "react-ionicons": "^3.0.1",
    "react-router-dom": "^5.0.0",
    "react-transition-group": "^2.8.0",
    "reactstrap": "^7.1.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "simple-oauth2": "^2.2.1"
