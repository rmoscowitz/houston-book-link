  "description": "Houston Hackathon 2017 Project",
  "main": "node/server/server.js",
  "scripts": {
    "preinstall": "cd react/houston-elibrary && npm set progress=false && npm install && cd ../../ && cd node && npm set progress=false && npm install",
    "postinstall": "pip install -r extractor/requirements.txt",
    "start-frontend": "cd react/houston-elibrary && npm start",
    "start-backend": "cd node && npm start",
    "start-extractor": "python extactor/main.py",
    "start-db-import": "python create_tables.py && python response_to_db.py && python create_text_search_vectors.py",
    "test": "echo \"Error: no test specified\" && exit 1"
