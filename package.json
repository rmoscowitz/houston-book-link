  "main": "server/index.js",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bookshelf": "^0.10.3",
    "bootstrap": "^4.0.0-alpha.6",
    "knex": "^0.13.0",
    "lodash": "^4.17.4",
    "pg": "^6.2.3",
    "react": "^15.5.4",
    "react-autosuggest": "^9.0.1",
    "react-dom": "^15.5.4",
    "react-foundation": "^0.7.0",
    "react-ionicons": "^1.5.7",
    "react-router-dom": "^4.1.1",
